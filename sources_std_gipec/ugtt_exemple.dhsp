procedure ug_graph
	recorddiva elem_G elem_G
	recorddiva elem_X elem_X
	recorddiva elem_Y elem_Y
	1 LstX L
	1 LstY L
	1 LstG L
	1 i 3,0

beginp

	lstX = ug_liste("LST_X")
	lstY = ug_liste("LST_Y")
	lstG = ug_liste("LST_GRAPH")

	;Base
	elem_x.Nom = sdate(today)
	listinsert(lstX,elem_x)
	elem_y.Nom = "0"
	listinsert(lstY,elem_Y)

	;Incrément de 10
	for i = 1 to 10
		elem_x.Nom = nospaces(sdate(hdate(today,i)))
		listinsert(lstX,elem_x)
		elem_y.Nom = str(i*10)
		listinsert(lstY,elem_Y)
	next
	
	mz.MeCle = "ugee_exemple.dhoe"
	xmeload(mz.MeCle)
	
	ug_Init_Graph(1,"LST_X","LST_Y")

	;Renseigne des valeurs de graphique

	;J a J+5 sur la ligne 20 : 100% X et Y
	init elem_G = system.Esp
	elem_G.Id = 1
	elem_G.Bulle = "J a J+5 sur la ligne 20 : 100% X et Y"
	elem_g.AxeDX = sdate(today)
	elem_g.AxeDY = "20"
	elem_g.AxeFX = sdate(hdate(today,5))
	elem_g.AxeFY = "20"
	listinsert(lstG,elem_g)
	
	;J+3 a J+3 sur les lignes 40 à 50 : 100% X et Y
	init elem_G = system.Esp
	elem_G.Id = 2
	elem_G.Bulle = "J+3 a J+3 sur les lignes 40 à 50 : 100% X et Y"
	elem_g.AxeDX = sdate(hdate(today,3))
	elem_g.AxeDY = "40"
	elem_g.AxeFX = sdate(hdate(today,3))
	elem_g.AxeFY = "50"
	listinsert(lstG,elem_g)

	;J+5 a J+5 sur les lignes 40 à 50 : 100% X et 30% sur début Y, 50 % sur fin Y
	init elem_G = system.Esp
	elem_G.Id = 3
	elem_G.Bulle = "J+5 a J+5 sur les lignes 40 à 50 : 100% X et 30% sur début Y, 50 % sur fin Y"
	elem_g.AxeDX = sdate(hdate(today,5))
	elem_g.AxeDY = "40"
	elem_g.AxeFX = sdate(hdate(today,5))
	elem_g.AxeFY = "50"
	elem_G.PcYD = 30
	elem_G.PcYF = 50
	listinsert(lstG,elem_g)

	;J+2 à J+7 sur la ligne 70: 30 % sur Début X, 50% sur fin X, 100% sur Y 
	init elem_G = system.Esp
	elem_G.Id = 4
	elem_G.Bulle = "J+2 à J+7 sur la ligne 70: 30 % sur Début X, 50% sur fin X, 100% sur Y"
	elem_g.AxeDX = sdate(hdate(today,2))
	elem_g.AxeDY = "70"
	elem_g.AxeFX = sdate(hdate(today,7))
	elem_g.AxeFY = "70"
	elem_g.PcXD = 30
	elem_g.PcXF = 50
	listinsert(lstG,elem_g)

	;J à J sur ligne 0 : 25 % sur tous les ratios
	init elem_G = system.Esp
	elem_G.Id = 5
	elem_G.Bulle = "J à J sur ligne 0 : 25 % sur tous les ratios"
	elem_g.AxeDX = sdate(today)
	elem_g.AxeDY = "0"
	elem_g.AxeFX = sdate(today)
	elem_g.AxeFY = "0"
	elem_g.PcXD = 25
	elem_g.PcXF = 25
	elem_G.PcYD = 25
	elem_G.PcYF = 25
	listinsert(lstG,elem_g)						 
	
	;J+10 ligne 0 à 100, 100%
	init elem_G = system.Esp
	elem_G.Id = 6
	elem_G.Bulle = "J+10 ligne 0 à 100, 100%"
	elem_g.AxeDX = sdate(hdate(today,10))
	elem_g.AxeDY = "0"
	elem_g.AxeFX = sdate(hdate(today,10))
	elem_g.AxeFY = "100"
	listinsert(lstG,elem_g)

	ug_Dessin_Graph_Histo("LST_GRAPH")

	getbchar(true)

endp
